{
  "server": {
    "_comment": {
      "server.maxRequestBodyBytes": "1024 * 1024 * 10 = 10MB"
    },
    "address": ":8080",
    "readTimeoutSec": 10,
    "writeTimeoutSec": 10,
    "idleTimeoutSec": 60,
    "requestTimeoutSec": 5,
    "maxRequestBodyBytes": 10485760
  },
  "logging": {
    "level": "debug",
    "dir": "logs"
  },
  "concurrency": {
    "maxConcurrentRequests": 5000,
    "mainLogicWorkerCount": 8,
    "dbWorkerCount": 4,
    "externalWorkerCount": 4,
    "inputChannelSize": 1024,
    "dbChannelSize": 256,
    "externalChannelSize": 256
  },
  "scheduler": {
    "timezone": "Asia/Seoul",
    "enabled": true
  },
  "configReload": {
    "enabled": true,
    "intervalMinutes": 10
  }
}
